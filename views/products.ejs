<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>

<body>
  <h1>Products</h1>
  <ul>
    <% products.forEach(product=> { %>
      <% if (product.price>= 0000) { %>
        <li>
          <a href="/products/<%= product.id %>">
            <%= product.name %> - <%= product.price %>
          </a>

        </li>
        <% } %>

          <% }) %>
  </ul>
</body>

</html>